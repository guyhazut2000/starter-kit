{
  "name": "starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=dot",
    "version:patch": "npm version patch -m \"chore(release): bump version to %s\" && git push --follow-tags",
    "version:minor": "npm version minor -m \"chore(release): bump version to %s\" && git push --follow-tags",
    "version:major": "npm version major -m \"chore(release): bump version to %s\" && git push --follow-tags",
    "version:prerelease": "npm version prerelease --preid=alpha -m \"chore(release): bump version to %s\" && git push --follow-tags",
    "version:show": "node -p \"require('./package.json').version\""
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "better-auth": "^1.4.0",
    "next": "16.1.2",
    "pg": "^8.13.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/pg": "^8.11.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.4.0",
    "prettier": "^3.8.0",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.4"
  }
}
